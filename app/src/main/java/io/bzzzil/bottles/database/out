        db.execSQL("insert into " + CountriesTable.TABLE_COUNTRIES + " values('TA', '" + BottlesApplication.getContext().getString(R.string.country_tristan_da_cunha) + "', " + R.drawable.flag_ta + ")");
        //db.endTransaction();
    }
}
